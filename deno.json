{
	"compilerOptions": {
		"strict": true,
		
		"noImplicitReturns": true,
		"noImplicitOverride": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noPropertyAccessFromIndexSignature": true
	},
	"tasks": {
		"dev": "deno run --check --watch main.ts"
	},
	"fmt": {
		"useTabs": true,
		"indentWidth": 4
	}
}
